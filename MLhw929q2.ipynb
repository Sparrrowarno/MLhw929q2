   "source": [
    "x = np.linspace(-5, 5, 100)\n",
    "y1 =(0.5)*np.abs(x-1)-np.abs(x)+np.log(2)\n",
    "\n",
    "plt.xlim(-6, 6)\n",
    " \n",
    "plt.plot(x, y1)\n",
    " \n",
    "plt.title('Log-likelihood-ratio function')\n",
    "plt.xlabel(r'$x$')\n",
    "plt.ylabel(r'$\\ell (x)$')\n",
    " \n",
    "plt.show()\n"
   ]
